{
	"name": "wikibase/query",
	"type": "mediawiki-extension",
	"description": "Extension to Wikibase Repo that adds the ability to define and execute queries",
	"keywords": [
		"Wikibase",
		"WikibaseQuery",
		"query",
		"Wikidata"
	],
	"homepage": "https://github.com/wmde/WikibaseQuery",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Developer"
		}
	],
	"support": {
		"irc": "irc://irc.freenode.net/wikidata"
	},
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "1.*,>=1.0.1",

		"data-values/data-values": "~0.1",
		"data-values/interfaces": "~0.1",

		"ask/ask": "~1.0",
		"ask/serialization": "~1.0.1",
		"serialization/serialization": "~3.0",

		"wikibase/data-model": "~1.0",
		"wikibase/wikibase": "@dev",
		"wikibase/query-engine": "~0.3.0@dev",
		"wikibase/entity-store": "~0.2.0@dev"
	},
	"require-dev": {
		"symfony/console": "~2.4"
	},
	"suggest": {
		"symfony/console": "Allows use of the command line interface"
	},
	"autoload": {
		"files" : [
			"WikibaseQuery.php"
		],
		"psr-4": {
			"Wikibase\\Query\\": "src/"
		}
	}
}
